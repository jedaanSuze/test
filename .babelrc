{
    "presets": [
        "react",
        "es2015",
        "es2016",
        "es2017"
    ],
    "plugins": [
        [
            "transform-runtime",
            {
                "polyfill": true,
                "regenerator": true
            }
        ],
        [
            "module-resolver",
            {
                "alias": {
                    "draft-js": "@wix/draft-js"
                }
            }
        ]
    ]
}
